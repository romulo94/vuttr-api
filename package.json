{
  "name": "template-node",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "predev": "sequelize db:migrate",
    "dev": "NODE_ENV=development nodemon -L src/server.js",
    "build": "sucrase ./src -d ./dist --transforms imports",
    "pretest": "NODE_ENV=test sequelize db:migrate",
    "test": "sh ./test.sh",
    "tests": "NODE_ENV=test jest --verbose --runInBand --detectOpenHandles --coverage  && cat ./__tests__/coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js",
    "posttest": "NODE_ENV=test sequelize db:migrate",
    "prepro": "NODE_ENV=production npm run build",
    "prod": "NODE_ENV=production node dist/server.js"
  },
  "dependencies": {
    "@sentry/node": "^5.6.2",
    "@sucrase/jest-plugin": "^2.0.0",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^8.0.0",
    "express": "^4.17.1",
    "express-async-errors": "^3.1.1",
    "jsonwebtoken": "^8.5.1",
    "pg": "^7.12.1",
    "pg-hstore": "^2.3.3",
    "sequelize": "^5.19.4",
    "youch": "^2.0.10"
  },
  "devDependencies": {
    "@types/jest": "^24.0.18",
    "coveralls": "^3.0.6",
    "eslint": "^5.16.0",
    "eslint-config-airbnb-base": "^13.2.0",
    "eslint-config-prettier": "^6.0.0",
    "eslint-plugin-import": "^2.18.0",
    "eslint-plugin-prettier": "^3.1.0",
    "factory-girl": "^5.0.4",
    "faker": "^4.1.0",
    "jest": "^24.9.0",
    "nodemon": "^1.19.1",
    "prettier": "^1.18.2",
    "sequelize-cli": "^5.5.0",
    "sqlite3": "^4.1.0",
    "sucrase": "^3.10.1",
    "supertest": "^4.0.2"
  }
}
